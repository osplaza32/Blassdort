<div class="middle-box text-center loginscreen  animated fadeInDown">
    <div>
      <%= image_tag "logos/blastdoorsinblanco.png",size: "450x450", alt: "BlastDoor" %>


    </div>
  <% if flash[:notice] %>
    <div class="notice"><%= flash[:notice] %></div>
  <% end %>
  <%=form_for :sessions, url: url_for( :controller => :sessions, :action => :auth ) do |f| %>

<div class="form-group">
      <%= email_field(:usuariomail, :email,class: 'form-control login') %>
      </div>
      <div class="form-group">
        <%= password_field_tag(:usuariopass,nil,class: 'form-control login') %>
      </div>
      <div class="form-check">
        <input class="form-check-input" type="checkbox" value="" id="defaultCheck1">
        <label class="form-check-label" for="defaultCheck1">
          <spam  class="link login-link"> Recordar mi Contraseña</spam>
        </label>
      </div>
      <button type="submit" class="btn btn-default block full-width m-b login-button">Login</button>

      <spam  class="link login-link" >¿Olvidaste tu contraseña? </spam>
    <%end%>
  </div>
</div>



